<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="448.0" prefWidth="667.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="423.0" prefWidth="400.0">
                     <children>
                        <VBox prefHeight="423.0" prefWidth="244.0">
                           <children>
                              <Label text="Sexo">
                                 <VBox.margin>
                                    <Insets left="20.0" right="20.0" top="20.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="sexo" onAction="#sexoChange" prefWidth="150.0">
                                 <VBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label text="Faixa Etária">
                                 <VBox.margin>
                                    <Insets left="20.0" right="20.0" top="20.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="faixaEtaria" onAction="#faixaEtariaChange" prefWidth="150.0">
                                 <VBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" />
                                 </VBox.margin>
                              </ComboBox>
                           </children>
                        </VBox>
                        <VBox fx:id="stack" prefHeight="423.0" prefWidth="236.0">
                           <children>
                              <Label text="Região">
                                 <VBox.margin>
                                    <Insets left="20.0" right="20.0" top="20.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="regiao" layoutX="10.0" layoutY="35.0" onAction="#regiaoChange" prefWidth="150.0">
                                 <VBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" />
                                 </VBox.margin>
                              </ComboBox>
                              <Label layoutX="10.0" layoutY="10.0" text="Classe Social">
                                 <VBox.margin>
                                    <Insets left="20.0" right="20.0" top="20.0" />
                                 </VBox.margin>
                              </Label>
                              <ComboBox fx:id="classesocial" layoutX="10.0" layoutY="60.0" onAction="#classeSocialChange" prefWidth="150.0">
                                 <VBox.margin>
                                    <Insets bottom="20.0" left="20.0" right="20.0" />
                                 </VBox.margin>
                              </ComboBox>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <TextArea fx:id="resultado" prefHeight="493.0" prefWidth="667.0" />
               </children>
            </VBox>
         </center></BorderPane>
   </children>
</GridPane>
